<?xml version="1.0" encoding="utf-8"?>
  <!--婚礼选择图片-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true" android:drawable="@mipmap/wedding_p"></item>
    <item android:state_selected="false" android:drawable="@mipmap/wedding" ></item>
    <item android:drawable="@mipmap/wedding_p" android:state_checked="true"/>
    <item android:drawable="@mipmap/wedding"  android:state_checked="false"/>
    <item android:state_focused="true" android:drawable="@mipmap/wedding_p"></item>
    <item android:state_focused="false" android:drawable="@mipmap/wedding"></item>
    <item android:state_pressed="true" android:drawable="@mipmap/wedding_p"></item>
    <item android:state_pressed="false" android:drawable="@mipmap/wedding"></item>
</selector>