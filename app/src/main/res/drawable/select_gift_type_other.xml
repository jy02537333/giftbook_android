<?xml version="1.0" encoding="utf-8"?>
  <!--婚礼选择图片-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true" android:drawable="@mipmap/other_p"></item>
    <item android:state_selected="false" android:drawable="@mipmap/other" ></item>
    <item android:drawable="@mipmap/other_p" android:state_checked="true"/>
    <item android:drawable="@mipmap/other"  android:state_checked="false"/>
    <item android:state_focused="true" android:drawable="@mipmap/other_p"></item>
    <item android:state_focused="false" android:drawable="@mipmap/other"></item>

</selector>